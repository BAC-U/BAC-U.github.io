  local descs = {'Highlighted Item ID','Cloned Item Flag','Power Level','Rarity','Gilded','Gilded Enchant','Gilded Level','Total Enchants','Max Enchants','Enchant 1-M','Enchant 1-R','Enchant 1-L','Enchant 2-M','Enchant 2-R','Enchant 2-L','Enchant 3-M','Enchant 3-R','Enchant 3-L','Enchant 1-M Tier','Enchant 1-R Tier','Enchant 1-L Tier','Enchant 2-M Tier','Enchant 2-R Tier','Enchant 2-L Tier','Enchant 3-M Tier','Enchant 3-R Tier','Enchant 3-L Tier','Total Traits','Max Traits','Trait 1','Trait 2','Trait 3','Trait 4','Trait 5','Trait 6','Trait 7','Trait 8'}
  for i=0,al.Count-1 do
    for o=1,#descs do
      if al[i].Description == descs[o] and al[i].address == 'p_equip' then
        for p=1,#savedItem do
          if al[i].Description == savedItem[p] then
            if savedItem[p+1] ~= '??' then
              al[i].value = savedItem[p+1]
            end
          end
        end
      end
    end
  end
end

