color = 0x000000
iterator = 0
ExitTimer = true

function colorChange()
  frequency = 20                      -- Rate at which it changes color... the higher the number, the faster the changes
  iterator = iterator + 0.001         -- i++
  red = math.floor(math.sin(frequency * iterator + (math.pi/4)) * 111 + 128)                               -- Generate R in Sinewave
  green = math.floor(math.sin(frequency * iterator + (3*math.pi/4)) * 111 + 128)                           -- Generate G in Sinewave
  blue = math.floor(math.sin(frequency * iterator + (5*math.pi/4)) * 111 + 128)                            -- Generate B in Sinewave
  --color = "0x"..string.format('%x',blue)..string.format('%x',green)..string.format('%x',red)     -- Strings up 0x?????? color in Hex
  color = (red)+(green&lt;&lt;0x8)+(blue&lt;&lt;0x10)
  if do_this ~= nil then                                        -- Checks if there's a Memrec to update yet
     if do_this ~= previous and previous ~= nil then            -- Checks if this is a new Memrec since last cycle
       previous.Color = o_color                                 -- Sets the previous memrec's color to its original color
       o_color = do_this.Color                                  -- Stores the new memrec's color for later
     end
     do_this.Color = color                                      -- Sets the current memrec's Color to the current iterated color
     previous = do_this                                         -- Stores the current memrec as 'previous' for next cycle
  end
  if ExitTimer == true and do_this ~= nil then                  -- Checks if we should stop cycling colors
    do_this.Color = o_color                                     -- Sets current memrec's color to its original color
    cycleTimer.destroy()                                        -- Stops the cycling timer
  end
end


function cycleColor(memRecord)                                  -- Decides what memrec should have its color updated
  if ExitTimer == false then
    al = getAddressList()                                       -- Get Address List
    for i=0, al.Count-1 do                                      -- Iterate through it
      if al[i].Description == memRecord then                    -- When finding a memrec with matching description...
        if previous == nil then                                 -- Check if it's the first iteration
          o_color = al[i].Color                                 -- If so, set its original color so it can be reset later
        end
        do_this = al[i]                                         -- Store the memrec
      end
    end
  else
    --print("Shouldn't change memrec while timer is off...")
  end
end





